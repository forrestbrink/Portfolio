﻿<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />

    <title>@ViewBag.Title</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="header">
        <header>
            <h1>Forrest Brink</h1>
            <nav>

                <a href="@Url.Action("Index", "Home")">Home</a>
                &nbsp || &nbsp
             <a href="@Url.Action("Index", "About")">About</a>
                &nbsp || &nbsp
             <a href="@Url.Action("Index", "Work")">Work</a>
                &nbsp || &nbsp
             <a href="@Url.Action("Index", "Contact")">Contact</a>

            </nav>
    </div>
    </header>
     <div id="body">
         @RenderSection("featured", required: false)
         <section id="container" class="content-wrapper main-content clear-fix">
             @RenderBody()
         </section>
     </div>
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
    <footer>
        <div class="footer">
            <p>© 2014 Forrest Brink</p>
            <nav>

                <a href="@Url.Action("Index", "Home")">Home</a>
                &nbsp || &nbsp
            <a href="@Url.Action("Index", "About")">About</a>
                &nbsp || &nbsp
            <a href="@Url.Action("Index", "Work")">Work</a>
                &nbsp || &nbsp
            <a href="@Url.Action("Index", "Contact")">Contact</a>

            </nav>
        </div>
    </footer>
</body>
</html>

